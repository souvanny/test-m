<br/>
<br/>

<table border="1">
    <tr>
        <th></th>
        <th>id</th>
        <th>marketplace</th>
        <th>idFlux</th>
        <th>orderStatusMarketplace</th>
        <th>orderStatusLengow</th>
        <th>orderId</th>
    </tr>

    {% for entity in orderList %}
        <tr>
            <td><a href="{{ path('order_view', { id: entity.id }) }}">View</a></td>
            <td>{{ entity.id }}</td>
            <td>{{ entity.marketplace }}</td>
            <td>{{ entity.idFlux }}</td>
            <td>{{ entity.orderStatusMarketplace }}</td>
            <td>{{ entity.orderStatusLengow }}</td>
            <td>{{ entity.orderId }}</td>
        </tr>
    {% endfor %}
</table>