<h3>List</h3>

<form action="" method="get">
id: <input size="10" type="text" value="" name="id" />
&nbsp;&nbsp;&nbsp;
marketplace: <input size="10" type="text" value="" name="marketplace" />
&nbsp;&nbsp;&nbsp;
idFlux: <input size="10" type="text" value="" name="idFlux" />
&nbsp;&nbsp;&nbsp;
orderStatusMarketplace: <input size="10" type="text" value="" name="orderStatusMarketplace" />
&nbsp;&nbsp;&nbsp;
orderStatusLengow: <input size="10" type="text" value="" name="orderStatusLengow" />
&nbsp;&nbsp;&nbsp;
orderId: <input size="10" type="text" value="" name="orderId" />
